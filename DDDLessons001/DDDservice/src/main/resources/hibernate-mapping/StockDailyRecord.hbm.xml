<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping default-access="field">
    <class name="ru.ddd.lessons.service.domain.model.StockDailyRecord" table="stock_daily_record"
           catalog="DDDLesson">
        <id name="id" type="java.lang.Integer">
            <column name="id"/>
            <generator class="identity"/>
        </id>
        <property name="priceOpen" type="java.lang.Float">
            <column name="PRICE_OPEN" precision="6"/>
        </property>
        <property name="priceClose" type="java.lang.Float">
            <column name="PRICE_CLOSE" precision="6"/>
        </property>
        <property name="priceChange" type="java.lang.Float">
            <column name="PRICE_CHANGE" precision="6"/>
        </property>
        <property name="volume" type="java.lang.Long">
            <column name="VOLUME"/>
        </property>
        <property name="date" type="date">
            <column name="DATE" length="10" not-null="false" unique="true"/>
        </property>
        <many-to-one name="stock" class="ru.ddd.lessons.service.domain.model.StockDailyRecord" fetch="select">
            <column name="stock_id" not-null="true"/>
        </many-to-one>
    </class>
</hibernate-mapping>